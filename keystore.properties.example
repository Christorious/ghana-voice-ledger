# Keystore configuration for release builds
# Copy this file to keystore.properties and fill in your actual values
# NEVER commit the actual keystore.properties file with real credentials

# Path to your release keystore file (relative to project root or absolute path)
# Example: keystore/ghana-voice-ledger-release.keystore
# Example: /Users/yourname/keystores/ghana-voice-ledger-release.keystore
RELEASE_KEYSTORE_FILE=your-keystore-file.jks

# Keystore password
RELEASE_KEYSTORE_PASSWORD=your-keystore-password

# Key alias
RELEASE_KEY_ALIAS=your-key-alias

# Key password
RELEASE_KEY_PASSWORD=your-key-password

# Additional signing configuration (optional)
# RELEASE_V1_SIGNING_ENABLED=true
# RELEASE_V2_SIGNING_ENABLED=true
# RELEASE_V3_SIGNING_ENABLED=true
# RELEASE_V4_SIGNING_ENABLED=true